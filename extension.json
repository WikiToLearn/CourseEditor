                "courseeditor-rename",
                "courseeditor-edit-dialog-section"
            ]
        },
        "ext.courseEditor.course": {
           "scripts":[
             "modules/courseEditor.js"
           ],
            "dependencies": [
                "oojs-ui"
            ],
            "styles": [
              "modules/styles/style.css"
            ],
            "messages": [
                "courseeditor",
                "courseeditor-add-new-section",
                "courseeditor-save-course",
                "courseeditor-cancel",
                "courseeditor-rename",
                "courseeditor-edit-dialog-course"
            ]
        }
